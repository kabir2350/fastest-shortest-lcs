#include <stdio.h>
#include <stdlib.h>
#include<string.h>
int main() {
    char x[]="bangladesg";
    char y[]="gladiator";//output=glad
    int s1= sizeof(x) / sizeof(x[0]);
    int s2= sizeof(y) / sizeof(y[0]);
    int i,j,arr[s1][s2];
    for(i=0;i<s1;i++){
        for(j=0;j<s2;j++)
            arr[i][j]=0;;
    }
    for(i=0;i<s1;i++){
        for(j=0;j<s2;j++)
        {   
            if(x[i]==y[j])arr[i+1][j+1]=1;
            printf(" %d \t", arr[i][j]);
        }
        printf("\n");
    }
    return 0;
}








